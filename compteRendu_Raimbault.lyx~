\begin_layout Plain Layout

## Question 5
\end_layout

\begin_layout Plain Layout

U = runif(10000,0,1) 
\end_layout

\begin_layout Plain Layout

V = runif(10000,0,1) 
\end_layout

\begin_layout Plain Layout

T = -2*log(U)
\end_layout

\begin_layout Plain Layout

X = sqrt(T)*cos(2*pi*V)
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

#i : loi de T ?
\end_layout

\begin_layout Plain Layout

n=length(T)
\end_layout

\begin_layout Plain Layout

plot(sort(T), 1:n/n, type="s", ylim=c(0,1), xlab="law of T", ylab="") 
\end_layout

\begin_layout Plain Layout

curve(1-exp(-x),add=TRUE,col="blue",ylab="")
\end_layout

\begin_layout Plain Layout

#ii : loi de X
\end_layout

\begin_layout Plain Layout

n=length(X)
\end_layout

\begin_layout Plain Layout

plot(sort(X), 1:n/n, type="s", ylim=c(0,1), xlab="law of X", ylab="")
\end_layout

\begin_layout Plain Layout

curve(from = -5,to = 5,0.5+atan(x)/pi,add=TRUE,col="blue",ylab="")
\end_layout

