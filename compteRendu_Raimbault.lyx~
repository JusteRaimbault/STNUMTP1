\begin_layout Plain Layout

#Description statistique
\end_layout

\begin_layout Plain Layout

summary(geyser)
\end_layout

\begin_layout Plain Layout

#Fonction de repartition
\end_layout

\begin_layout Plain Layout

par(mfcol=c(2,1),bg="lightyellow")
\end_layout

\begin_layout Plain Layout

n=length(geyser$waiting)
\end_layout

\begin_layout Plain Layout

plot(sort(geyser$waiting), 1:n/n, type="s", ylim=c(0,1), xlab="", ylab="",main="
law for waiting time")
\end_layout

\begin_layout Plain Layout

n=length(geyser$duration)
\end_layout

\begin_layout Plain Layout

plot(sort(geyser$duration), 1:n/n, type="s", ylim=c(0,1), xlab="", ylab="",main=
"law for duration")
\end_layout

